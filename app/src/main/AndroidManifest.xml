<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ascendcorparation.ascend4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".menu">
            <intent-filter>
                <action android:name="android.intent.action.menu" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".instruction">
            android:label="@string/INSTRUCTION"
        </activity>
        <activity android:name=".play">
        android:label="@string/PLAY"
        </activity>
        <activity android:name=".statistics">
            android:label="@string/STATISTICS"
    </activity>
        <activity android:name=".score">
            android:label="@string/SCORE"
        </activity>
    </application>

</manifest>
